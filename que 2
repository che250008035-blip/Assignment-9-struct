#include <iostream>
using namespace std;
struct color{
  int red;
  int green;
  int blue;
};
int main(){
  color colors[5]  {
      { 267,56,87},
      {78,84,92},
      {453,246,98},
      {321,743,76},
      {89,76,278}
  };
  for (int i=0;i<3;++i){
      colors[i].red=255-colors[i].red;
      colors[i].green=255-colors[i].green;
      colors[i].blue=255-colors[i].blue;
  }
  cout<<"new RGB values after inversion:"<<endl;
  for(int i=0;i<5;++i){
      cout<<"color:"<<(i+1)
      <<" red :"<<colors[i].red
      <<" green:"<<colors[i].green
      <<" blue:"<<colors[i].blue<<endl;
  }
    
    
    
    
    return 0;
}
