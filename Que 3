#include <iostream>
using namespace std;
struct point{
    float x;
    float y;
};
int main(){
point points[7];
    int count=0;
    cout <<"enter x and y coordinates for 7 points :"<<endl;
    for (int i=0;i<7;++i){
        cout<< "point:"<< (i+1)<<"(x,y):";
        cin>>points[i].x>>points[i].y ;
    
}
  for (int i=0;i<7;++i){
      if(points[i].x>0&& points[i].y>0) 
      count++;
}
cout <<" the number of points in 1st quadrant:"<<count<<endl;



    
    
    
    return 0;
}
